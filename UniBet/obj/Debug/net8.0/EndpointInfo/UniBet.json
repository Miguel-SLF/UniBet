          "depositAmount": {
            "type": "number",
            "format": "float"
          },
          "depositType": {
            "type": "string",
            "nullable": true
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "GameDTO": {
